
@{
    ViewBag.Title = "UserAct";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
   .actTbl{
       border:1px;
       width:40%;
      height:150px;
      border-color:#6B8E23;
   }
   .actTbl tr{
       border-color:#6B8E23;
   }
   .activitytd{
       height:10px;
       background-color:#6B8E23;
   }
    .fromtd, .totd {
        width: 25%;
        height: 30px;
        background-color: #6B8E23;
    }
    .datepickertd, .datetotd {
        width: 26%;
        height: 30px;
        background-color: #6B8E23;
        vertical-align: middle;
    }
   .btnleft_td{
    width:25%;
    height:30px;
    background-color:#6B8E23;
   }
   .btntd{
       width:26%;
       height:30px;
       background-color:#6B8E23;
   }
   .userdtltbl{
       border:1px;
       border-color:#6B8E23;
       margin-top:5px;
   }
   .userdtltbl .usertd{
       width:160px;
       background-color:#6B8E23;
        border-color:#000000;
   }
   .medicaltd{
        width:130px;
       background-color:#6B8E23;
        border-color:#000000;
   }
   .usertdwhite{
       width:160px;
       background-color:white;
       border-color:#000000;
   }
   .medicaltdwhite{
        width:130px;
       background-color:white;
        border-color:#000000;
   }
</style>
    <table class="actTbl" align="center" cellpadding="0" >
        <tr>
            <td class="activitytd" colspan="3">
                <div align="center"><font color="#FFFFFF" size="4">User Activity</font></div>
                </td>
        </tr>
        <tr>
            <td class="fromtd" align="left">
                <div align="left"><b><font color="#FFFFFF">FROM:</font></b></div></td>
            <td class="datepickertd" align="center" valign="middle">
                <div align="center">
                   
                </div>
            </td>
        </tr>
        <tr>
            <td class="totd" align="left">
                <div align="left"><b><font color="#FFFFFF">TO:</font></b></div>
            <td class="datetotd" align="center" valign="middle">
                <div align="center">
                    
                </div>
            </td>
        </tr>
        <tr>
            <td align="left" class="btnleft_td">
                <div align="left"><b><font color="#FFFFFF"></font></b></div>
            <td class="btntd"  valign="middle" align="center">
                <div align="center">
                    <input name="Reset" TYPE="Reset" VALUE="Reset">
                    <input name="Submit" TYPE="Submit" VALUE="Submit">
                </div>
            </td>
        </tr>
    </table>


    <table class="userdtltbl" col="4" cellspace="10" align="center">
        <tr>
            <td class="usertd">
                <div align="center"><b><font color="#FFFFFF">User </font></b></div>
            </td>

            <td class="usertd">
                <div align="center"><b><font color="#FFFFFF">Date/Time</font></b></div>
            </td>

            <td class="medicaltd">
                <div align="center"><b><font color="#FFFFFF">Medical Record#</font></b></div>
            </td>

            <td class="medicaltd">
                <div align="center"><b><font color="#FFFFFF">Serial Number</font></b></div>
            </td>

            <td class="usertd">
                <div align="center"><b><font color="#FFFFFF">Patient Name</font></b></div>
            </td>
        </tr>
        <tr>
            <td class="usertdwhite">
                <div align="left"><font color="black"></font></div>
            </td>

            <td class="usertdwhite">
                <div align="left"><font color="black"></font></div>
            </td>

            <td class="medicaltdwhite">
                <div align="left"><font color="black"> </font></div>
            </td>

            <td  class="medicaltdwhite">
                <div align="left"> <font color="black"></font></div>
            </td>

            <td class="usertdwhite">
                <div align="left"> <font color="black"></font></div>
            </td>
        </tr>

    </table>



